   ],
   "source": [
    "def split_and_join(line):\n",
    "    # write your code here\n",
    "    return '-'.join(line.split(' '))\n",
    "if __name__ == '__main__':\n",
    "    line ='this is a string'\n",
    "    result = split_and_join(line)\n",
    "    print (result)"
